(function($) {
	$.fn.myaccordion = function(opts) {
		var settings = $.extend({
			selectedClz:"navSelected",
			titleTagName:"h3"
		},opts||{});  
		$(this).find(settings.titleTagName).css("cursor","pointer");
		$(this).find("ul>"+settings.titlgTagName) .nextAll().css("display","none");;
		$(this).find("ul."+settings.selectedClz).find(settings.titleTagName).nextAll().css("display","block");;
	}; 
})(jQuery);
