(function($) {
	$.fn.myaccordion = function(opts) {
		var settings = $.extend({
			selectedClz:"navSelected",
			titleTagName:"h3"
		},opts||{});  
		$(this).find(settings.titleTagName).css("cursor","pointer");
		$(this).find("ul").find(titleTagName).nextAll().css("display","block");
	}; 
})(jQuery);
